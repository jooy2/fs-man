{
  "name": "fsman",
  "version": "0.0.5",
  "description": "A file system utility that can be used with Node.js fs module.",
  "type": "module",
  "types": "dist/global.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && npm run build && mocha --parallel test/*.spec.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jooy2/fsman"
  },
  "keywords": [
    "file",
    "fs",
    "file management",
    "files",
    "explorer",
    "directory",
    "folder",
    "verify",
    "path",
    "windows",
    "macos",
    "unix",
    "linux"
  ],
  "author": "Jooy2 <jootc.help@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "winattr": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^9.2.0",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
